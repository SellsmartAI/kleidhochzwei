        const typInput = document.getElementById('typ');
        const anlInput = document.getElementById('anl');
    
        function updateAnlInput() {
            // Check if the "typ" input contains any value
            if (typInput.value.trim() !== '') {
                // If it contains a value, set "x" in the "anl" input
                anlInput.value = 'x';
            } else {
                // If it doesn't contain a value, set "-" in the "anl" input
                anlInput.value = '-';
            }
        }
    
        // Add an event listener to the "typ" input to trigger the update when it changes
        typInput.addEventListener('input', updateAnlInput);
    
        // Initial update of the "anl" input based on the initial value of "typ" input
        updateAnlInput();
